# Core model keys
OPENAI_API_KEY=
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_ENDPOINT=
ANTHROPIC_API_KEY=
OPENROUTER_API_KEY=sk-or-your-key

# Model selection
MODEL_PROVIDER=openrouter
OPENAI_MODEL=gpt-4o-mini
ANTHROPIC_MODEL=claude-3-sonnet-20240229
OPENROUTER_MODEL=deepseek/deepseek-coder-v2-instruct
OPENROUTER_BASE=https://openrouter.ai/api/v1

# Embeddings
EMBEDDING_PROVIDER=openrouter
EMBEDDING_MODEL=openai/text-embedding-3-large

# LangSmith / tracing
LANGCHAIN_TRACING_V2=false
LANGCHAIN_PROJECT=langgraph-poc
LANGCHAIN_ENDPOINT=

# Vector / memory
VECTOR_DB_IMPL=qdrant
VECTOR_DB_PATH=./data/memory/vectorstore
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
QDRANT_COLLECTION=langgraph_memories
MEMORY_TIME_WINDOW_DAYS=30
MEMORY_DECAY_HALF_LIFE_HOURS=72
MEMORY_TOP_K=8
MEMORY_TTL_TASK_DAYS=7
ALLOW_MEMORY_WRITE=true
SCRUB_TRAJECTORIES=false

# MCP / integrations
MCP_ENABLED=true
MCP_SERVER_REGISTRY=./configs/mcp_tools.yaml

# Runner + logging
LOG_LEVEL=INFO
TRAJECTORY_DIR=./data/trajectories
